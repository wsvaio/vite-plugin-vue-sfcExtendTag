{
  "name": "vite-plugin-vue-sfcextendtag",
  "version": "0.0.10",
  "description": "支持.vue文件<template>根标签添加tag等属性",
  "types": "./types/index.d.ts",
  "main": "./lib/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "lib",
    "types"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./dist/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:ESNext && npm run build:CommonJS",
    "build:ESNext": "tsc -m ESNext  --outDir dist",
    "build:CommonJS": "tsc -m CommonJS --outDir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/vite-plugin-vue-sfcExtendTag.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "vue",
    "sfc",
    "extend",
    "tag"
  ],
  "author": "wsvaio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wsvaio/vite-plugin-vue-sfcExtendTag/issues"
  },
  "homepage": "https://github.com/wsvaio/vite-plugin-vue-sfcExtendTag#readme",
  "devDependencies": {
    "@types/node": "^18.0.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  }
}